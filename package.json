{
  "name": "public-archive-service-sdk",
  "version": "0.0.1",
  "description": "Your history, forever. Public Archive Service enables a truly decentralized Social History Service. This is a SDK to support sending transactions and reading data according to the Public Archive Service schema for the Symbol blockchain.",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.ts",
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/public-archive-service/sdk-typescript.git"
  },
  "keywords": [
    "public-archive-service",
    "blockchain",
    "symbol",
    "sdk"
  ],
  "author": "Public Archive Service",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/public-archive-service/sdk-typescript/issues"
  },
  "homepage": "https://github.com/public-archive-service/sdk-typescript#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "rxjs": "^7.4.0",
    "symbol-sdk": "^1.0.3"
  }
}
